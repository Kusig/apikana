{
  "name": "apikana",
  "version": "0.1.3",
  "description": "Integrated tools for REST API design - ｱﾋﾟ",
  "main": "index.js",
  "bin": {
    "apikana": "./bin/apikana",
    "apikana-serve": "./bin/apikana-serve"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify src/browserify.js -t brfs -i source-map-support > lib/browserify.js",
    "test-build": "node bin/apikana test test-dist --javaPackage=apikana.test --env=dev",
    "test-deploy": "node bin/apikana test test-deploy --javaPackage=apikana.test --env=dev --deploy",
    "test-serve": "node bin/apikana-serve test test-dist --port=8222 --env=dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbovet/apikana.git"
  },
  "keywords": [
    "REST",
    "API",
    "design",
    "documentation",
    "contract-first"
  ],
  "author": "Laurent Bovet",
  "contributors": [
    "Stefan Niederhauser"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lbovet/apikana/issues"
  },
  "homepage": "https://github.com/lbovet/apikana#readme",
  "dependencies": {
    "chalk": "1.1.3",
    "fs-extra": "2.0.0",
    "gulp": "3.9.1",
    "gulp-inject": "4.2.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-util": "3.0.8",
    "node-http-server": "6.2.1",
    "object-path": "0.11.3",
    "swagger-ui": "2.2.10",
    "through2": "2.0.3",
    "traverse": "0.6.6",
    "typescript": "2.1.5",
    "typescript-json-schema": "0.9.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "yamljs": "0.2.8"
  },
  "devDependencies": {
    "brfs": "1.4.3",
    "browserify": "13.0.0",
    "docson": "1.0.2"
  }
}
